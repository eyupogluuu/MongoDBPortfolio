	@* <script src="~/sidebar-05/js/jquery.min.js"></script> *@
	<script src="~/sidebar-05/js/popper.js"></script>
	<script src="~/sidebar-05/js/bootstrap.min.js"></script>
	<script src="~/sidebar-05/js/main.js"></script>
